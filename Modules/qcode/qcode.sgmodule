#!name=QCode API User-Agent Modifier
#!desc=Modify User-Agent header for asia.qcode.cc API requests
#!system=ios

[Script]
QCode UA Modifier = type=http-request, pattern=^https://asia\.qcode\.cc/api, script-path=https://raw.githubusercontent.com/MikotoZero/SurgeConfigs/master/Modules/qcode/qcode.js, requires-body=false, timeout=10

[MITM]
hostname = %APPEND% asia.qcode.cc